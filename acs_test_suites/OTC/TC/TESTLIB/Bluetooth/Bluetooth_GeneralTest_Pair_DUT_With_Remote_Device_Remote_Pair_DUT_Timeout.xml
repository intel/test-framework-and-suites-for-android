            <Name>script-args</Name>
            <Value> action_dut=Timeout action_dev=Pair action_initiator_first=true</Value>
        </Parameter>
        <Parameter>
            <Name>timeout</Name>
            <Value>10000</Value>
        </Parameter>
    </Parameters>
</TestCase>
